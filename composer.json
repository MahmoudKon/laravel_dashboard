{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-elfinder": "^0.5.2",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "joedixon/laravel-translation": "^2.2",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "mahmoudkon/messenger": "dev-master",
        "mcamara/laravel-localization": "^1.8",
        "psr/simple-cache": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "realrashid/sweet-alert": "^6.0",
        "spatie/laravel-backup": "^8.2",
        "spatie/laravel-permission": "^5.9",
        "spatie/laravel-translatable": "^6.3",
        "yajra/laravel-datatables": "^10.0",
        "yajra/laravel-datatables-buttons": "^10.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/EmailHelper.php",
            "app/Helpers/CrudGeneratorHelper.php",
            "app/Constants/Constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
